{
  "version": "0.0.3",
  "private": true,
  "main": "src/main.ts",
  "scripts": {
    "postinstall": "patch-package",
    "prebuild": "rimraf dist",
    "build": "node --max_old_space_size=8048 node_modules/@nestjs/cli/bin/nest build",
