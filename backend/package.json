    "start": "node --max-old-space-size=8048 dist/main",
    "test": "jest --testTimeout 5000000 --config jestconfig.json --verbose --forceExit",
    "test:dev": "jest --watch --testTimeout 5000000 --config jestconfig.json --verbose --forceExit",
    "prettier": "prettier --config .prettierrc \"./**/*.{ts,tsx,js,scss}\" --write",
    "tslint:check": "tslint --project ."
  },
